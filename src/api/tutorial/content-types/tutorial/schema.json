{
  "kind": "collectionType",
  "collectionName": "tutorials",
  "info": {
    "singularName": "tutorial",
    "pluralName": "tutorials",
    "displayName": "Tutorial",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "minLength": 5,
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "type": {
      "type": "enumeration",
      "enum": ["video", "text"],
      "default": "video"
    },
    "url": {
      "type": "string"
    },
    "contents": {
      "type": "text"
    },
    "summary": {
      "type": "text",
      "maxLength": 200
    },
    "classroom": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::classroom.classroom",
      "inversedBy": "tutorials"
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    }
  }
}
